<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <declare-styleable name="OwnToolBar">
        <attr name="layoutType" format="enum">
            <enum name="title_center" value="0" />
            <enum name="title_left" value="1" />
        </attr>
        <attr name="layoutBackground" format="color" />
        <attr name="layoutTitle" format="string" />
        <attr name="layoutTitleColor" format="color" />
    </declare-styleable>
    <declare-styleable name="MaterialSpinner" tools:ignore="ResourceName">
        <attr name="ms_arrow_tint" format="color"/>
        <attr name="ms_hide_arrow" format="boolean"/>
        <attr name="ms_background_color" format="color"/>
        <attr name="ms_text_color" format="color"/>
    </declare-styleable>
</resources>
